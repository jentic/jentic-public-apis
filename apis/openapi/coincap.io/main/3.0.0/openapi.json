    "/v3/assets/{slug}/history": {
      "get": {
        "summary": "Retrieve historical data for a specific asset",
        "description": "Retrieve historical data for a specific asset by slug (id)",
        "tags": [
          "Assets"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "slug",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The slug of the asset"
          },
          {
            "in": "query",
            "name": "interval",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "interval choices m1 m5 m15 m30 h1 h2 h6 h12 d1"
          },
          {
            "in": "query",
            "name": "start",
            "schema": {
              "type": "integer"
            },
            "description": "UNIX time in milliseconds. Omitting will return the most recent asset history. If start is supplied, end is required and vice versa."
          },
          {
            "in": "query",
            "name": "end",
            "schema": {
              "type": "integer"
            },
            "description": "The end timestamp for the historical data."
          }
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Historical data for the asset",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "timestamp": {
                      "type": "integer",
                      "description": "Unix timestamp when the data was retrieved",
                      "example": 1726081715628
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "priceUsd": {
                            "type": "string",
                            "description": "The price in USD at the specified time",
                            "example": "57006.4814312680237124"
                          },
                          "time": {
                            "type": "integer",
                            "description": "Unix timestamp of the price data",
                            "example": 1725915600000
                          },
                          "date": {
                            "type": "string",
                            "format": "date-time",
                            "description": "ISO 8601 formatted date and time of the price data",
                            "example": "2024-09-09T21:00:00.000Z"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Asset not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "bitcoin not found"
                    },
                    "timestamp": {
                      "type": "integer",
                      "description": "Unix timestamp when the error occurred"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/v3/exchanges": {
      "get": {
        "summary": "Retrieve a list of exchanges",
        "description": "Retrieve a list of exchanges with optional pagination",
        "tags": [
          "Exchanges"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "integer"
            },
            "description": "Number of results to return (default is 10)"
          },
          {
            "in": "query",
            "name": "offset",
            "schema": {
              "type": "integer"
            },
            "description": "Number of results to skip (default is 0)"
          }
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "A list of exchanges",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "description": "List of exchanges",
                      "items": {
                        "type": "object",
                        "properties": {
                          "exchangeId": {
                            "type": "string",
                            "description": "The unique ID of the exchange",
                            "example": "itbit"
                          },
                          "name": {
                            "type": "string",
                            "description": "The name of the exchange",
                            "example": "Itbit"
                          },
                          "rank": {
                            "type": "string",
                            "description": "The rank of the exchange",
                            "example": "33"
                          },
                          "percentTotalVolume": {
                            "type": "string",
                            "nullable": true,
                            "description": "The percentage of total market volume handled by the exchange",
                            "example": null
                          },
                          "volumeUsd": {
                            "type": "string",
                            "nullable": true,
                            "description": "The total volume in USD traded on the exchange",
                            "example": null
                          },
                          "tradingPairs": {
                            "type": "string",
                            "description": "The number of trading pairs available on the exchange",
                            "example": "0"
                          },
                          "socket": {
                            "type": "boolean",
                            "nullable": true,
                            "description": "Whether the exchange supports WebSocket",
                            "example": null
                          },
                          "exchangeUrl": {
                            "type": "string",
                            "description": "The URL of the exchange",
                            "example": "https://www.itbit.com/"
                          },
                          "updated": {
                            "type": "integer",
                            "description": "The last updated timestamp",
                            "example": 0
                          }
                        }
                      }
                    },
                    "timestamp": {
                      "type": "integer",
                      "description": "Unix timestamp when the data was retrieved",
                      "example": 1726084388658
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid query parameters",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "Invalid query parameters"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "Internal server error"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
