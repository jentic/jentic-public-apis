{
  "openapi": "3.0.0",
  "info": {
    "title": "Coincap OpenAPI",
    "version": "3.0.0",
    "x-jentic-source-url": "https://rest.coincap.io/api-docs.json"
  },
  "servers": [
    {
      "url": "https://rest.coincap.io"
    }
  ],
  "paths": {
    "/v3/assets": {
      "get": {
        "summary": "Retrieve a list of assets",
        "description": "Retrieve a list of assets with optional filters and pagination.",
        "tags": [
          "Assets"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "search",
            "schema": {
              "type": "string"
            },
            "description": "search by asset slug (bitcoin) or symbol (BTC)"
          },
          {
            "in": "query",
            "name": "ids",
            "schema": {
              "type": "string"
            },
            "description": "Comma-separated list of asset ids (aka slugs). query with multiple ids=bitcoin,ethereum,monero"
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "integer"
            },
            "description": "Number of results to return (default is 100)"
          },
          {
            "in": "query",
            "name": "offset",
            "schema": {
              "type": "integer"
            },
            "description": "Number of results to skip (default is 0)"
          }
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "A list of assets",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "timestamp": {
                      "type": "integer",
                      "description": "Unix timestamp when the data was retrieved",
                      "example": 1726081635506
                    },
                    "data": {
                      "type": "array",
                      "description": "The list of assets.",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string",
                            "description": "The ID or slug of the asset",
                            "example": "bitcoin"
                          },
                          "rank": {
                            "type": "string",
                            "description": "The rank of the asset",
                            "example": "1"
                          },
                          "symbol": {
                            "type": "string",
                            "description": "The symbol of the asset",
                            "example": "BTC"
                          },
                          "name": {
                            "type": "string",
                            "description": "The full name of the asset",
                            "example": "Bitcoin"
                          },
                          "supply": {
                            "type": "string",
                            "description": "Total supply of the asset",
                            "example": "19752815.0000000000000000"
                          },
                          "maxSupply": {
                            "type": "string",
                            "description": "The max supply of the asset, or 0 if unlimited",
                            "example": "21000000.0000000000000000"
                          },
                          "marketCapUsd": {
                            "type": "string",
                            "description": "The market capitalization in USD",
                            "example": "1134508584478.0989721079862315"
                          },
                          "volumeUsd24Hr": {
                            "type": "string",
                            "description": "The 24-hour volume in USD",
                            "example": "7243846863.3409126543165751"
                          },
                          "priceUsd": {
                            "type": "string",
                            "description": "The current price in USD",
                            "example": "57435.2862859343831301"
                          },
                          "changePercent24Hr": {
                            "type": "string",
                            "description": "The percentage change in price in the last 24 hours",
                            "example": "-0.0461491427646531"
                          },
                          "vwap24Hr": {
                            "type": "string",
                            "description": "Volume Weighted Average Price (VWAP) over the last 24 hours",
                            "example": "57868.1484672081301126"
                          },
                          "explorer": {
                            "type": "string",
                            "description": "URL to a block explorer for the asset",
                            "example": "https://blockchain.info/werweqrerwerw"
                          },
                          "tokens": {
                            "type": "object",
                            "description": "Mapping of chain IDs to token address arrays. May be empty if no tokens found.",
                            "additionalProperties": {
                              "type": "array",
                              "items": {
                                "type": "string"
                              }
                            },
                            "example": {
                              "1": [
                                "0xc02aaa39b223fe8d0a0e5c4f27ead9083c756cc2"
                              ],
                              "10": [
                                "0x4200000000000000000000000000000000000006"
                              ],
                              "137": [
                                "0x7ceb23fd6bc0add59e62ac25578270cff1b9f619"
                              ],
                              "42161": [
                                "0x82af49447d8a07e3bd95bd0d56f35241523fbab1"
                              ]
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
